{
  "compilerOptions": {
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "keyofStringsOnly": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "lib": [
      "es2017",
      "dom"
    ],
    "target": "es6",
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
